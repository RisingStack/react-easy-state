{
  "setupFilesAfterEnv": ["./scripts/testSetup.js"],
  "testURL": "http://react-easy-state.com",
  "testRegex": "\\.test\\.jsx?$",
  "collectCoverage": true,
  "coverageReporters": ["lcovonly", "text"],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "examples/**/*.{js,jsx}",
    "!examples/**/build/**",
    "!examples/**/index.js",
    "!examples/stop-watch/**",
    "!examples/beer-finder/**",
    "!examples/native-clock/**"
  ],
  "coverageDirectory": "coverage"
}
